<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:ignore="UnusedAttribute">

    <font
        android:font="@font/roboto_regular"
        android:fontStyle="normal"
        android:fontWeight="400"
        app:font="@font/roboto_regular"
        app:fontStyle="normal"
        app:fontWeight="400" />

    <font
        android:font="@font/roboto_italic"
        android:fontStyle="italic"
        android:fontWeight="400"
        app:font="@font/roboto_italic"
        app:fontStyle="italic"
        app:fontWeight="400" />

    <font
        android:font="@font/roboto_light"
        android:fontStyle="normal"
        android:fontWeight="700"
        app:font="@font/roboto_light"
        app:fontStyle="normal"
        app:fontWeight="700" />

    <font
        android:font="@font/roboto_medium"
        android:fontStyle="italic"
        android:fontWeight="700"
        app:font="@font/roboto_medium"
        app:fontStyle="italic"
        app:fontWeight="700" />

</font-family>